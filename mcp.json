{
  "mcpServers": {
    "context7-docusaurus": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@context7/mcp-docusaurus"],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}",
        "DOCUSAURUS_PATH": "./mmni"
      }
    },
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    }
